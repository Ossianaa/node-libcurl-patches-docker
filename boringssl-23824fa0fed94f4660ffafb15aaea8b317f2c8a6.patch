+  if (!str) {
+    return 0;
+  }
+  bssl::Array<uint8_t> arr;
+  arr.Init(strlen(str));
+  int i = 0;
+  const char *l = str;
+  do {
+    arr[i++] = *l - 1;
+  } while (*++l != '\0');
+  ctx->extension_permutation = std::move(arr);
+  return ctx->extension_permutation.empty() ? 0 : 1;
