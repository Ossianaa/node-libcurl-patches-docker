+int SSL_CTX_set_ciphersuites(SSL_CTX *ctx, char *str) {
+  if (!str) {
+    return 0;
+  }
+  bssl::Array<uint8_t> arr;
+  arr.Init(strlen(str));
+  int i = 0;
+  const char *l = str;
+  do {
+    arr[i++] = *l;
+  } while (*++l != '\0');
+  ctx->cipher13_flag_list = std::move(arr);
+  return ctx->cipher13_flag_list.empty() ? 0 : 1;
+}
